{
  "parser": "babel-eslint",
  "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
          "jsx": true
      }
  },
  "rules": {
    "strict": 0,
    "max-len": [1, 180],
    "no-console": 1,
    "indent": ["error", 4]
  },
  "extends": [
    "plugin:flowtype/recommended",
    "walmart/configurations/es6-node"
  ],
  "plugins": [
    "flowtype"
  ]
}
